{
    "manifest_version": 2,
    "name": "CyboMonitor",
    "version": "1.0",
    "description": "Cybozeへの書き込みを監視",
    "icons": { "48": "icons/icon_048.png",
              "128": "icons/icon_128.png" },
    "background": {
      "scripts": ["js/jquery.min.js", "js/background.js"]
    },
    "browser_action" : {
      "default_icon" : "icons/icon.png",
      "default_title" : "Popup",
      "default_popup" : "popup.html"
    },
    "options_page": "options.html"
}
